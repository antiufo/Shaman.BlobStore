    "keyFile": "ShamanOpenSourceKey.snk"
  },
  "exclude": [
    "Properties/AssemblyInfo.cs"
  ],
  "frameworks": {
    "net35": {},
    "net45": {
      "frameworkAssemblies": {
        "System.Runtime": "4.0.0.0"
      }
    },
    "dotnet5.4": {
      "compilationOptions": {
        "define": [
          "CORECLR"
        ]
      },
      "dependencies": {
        "System.Runtime": "4.0.21-beta-23428",
        "System.Runtime.Extensions": "4.0.11-beta-23217",
        "System.IO.FileSystem": "4.0.1-beta-23217",
        "System.IO": "4.0.11-beta-23217",
        "System.Diagnostics.Debug": "4.0.11-beta-23225",
        "System.Collections": "4.0.11-beta-23217",
        "System.Linq": "4.0.1-beta-23217",
        "System.Text.Encoding": "4.0.11-beta-23217",
        "System.Threading": "4.0.11-beta-23217",
        "System.Console": "4.0.0-beta-23217"
      }
    }
  },
  "dependencies": {
    "Shaman.Configuration.Metadata": "1.0.0-beta-4922"
  },
  "runtimes": {
    "win": {}
  }
}